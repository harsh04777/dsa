class Solution {
public:
    void rotate(vector<int>& nums, int k) {

     k%=nums.size();
        reverse(nums.begin(),nums.end());//pehle pure array ko reverse
        reverse(nums.begin(),nums.begin()+k);// fir k nos ko 
        reverse(nums.begin()+k,nums.end());//bachey huey no
        
        
        
    }
};
