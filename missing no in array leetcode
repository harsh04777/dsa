class Solution {
public:
    int missingNumber(vector<int>& nums) {
        int sum=0;
        for(int i=0;i<nums.size();i++){
            sum+=nums[i];// sum find karenge
        }
        int n=nums.size();
        int totalsum=n*(n+1)/2;total sum find karenge from basic math formula
        return totalsum-sum; //minus kardenge.
        
    }
};
