void rotate(int arr[], int n)
{
    int temp= arr[n-1];  // temp m array ka last value store krenge
    for(int i=n-1;i>=1;i--){//array ko piche se chalayenge taaaki 
    
        arr[i]=arr[i-1];// array ki current value m pichli value store hojaye ,clockwise stire karna h
    
    }
    arr[0]=temp;//last m array k first value par last wala value store krdenge
    
}
